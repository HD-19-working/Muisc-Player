/* components/VideoItem/VideoItem.wxss */
.mv_box{
  font-size: 12px;
  font-family: "微软正黑体";
  color: whitesmoke;
  padding: 5px;
  background-color: #282828;
  border-radius: 15px;
  margin-bottom: 10px;
}
.video_image_box{
  position: relative;
}
.video_image{
  width: 100%;
  border-radius: 12px;
  /* box-shadow: 1px 1px 4px 2px #A0a4a4; */
  overflow: hidden;
  box-shadow: 0 0 6px 3px #161616;
}
.video_text{
  padding: 5px 5px;
}
.content_text{

  overflow: hidden;
  text-overflow:ellipsis; /* 文本超出呈现方式：省略号 */
  display:-webkit-box; /* 盒模型 */
  -webkit-line-clamp:2; /* 限制文本行数 */
  -webkit-box-orient:vertical; /* 盒内子元素布局方向 */
  font-size: 14px;
  line-height: 18px;
}
.creator_text{
  color:#C0C0C0;
  margin-top: 5px;
}
.video_tag{
  position: absolute;
  color: white;
  width: 100%;
  height: 100%;
  top: 0;
  padding: 8px 8px;
  box-sizing: border-box;
  background: linear-gradient(transparent 60%,rgba(18,18,18,0.5));
}
.text_box{
  position: absolute;
  bottom: 8px;
  display: flex;
  width: calc(100% - 16px);
  justify-content: space-between;
}
.playcount_tag{
  padding-left: 20px;
  background-size: 18px;
  background-position-x: 0;
  background-position-y: center;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}